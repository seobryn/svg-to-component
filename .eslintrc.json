{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-standard",
    "globals": {
        "__dirname": true,
        "process": true
    },
    "rules": {
        "no-param-reassign": ["error", {
            "props": true,
            "ignorePropertyModificationsFor": [
                "options"
            ]
        }],
        "no-console":"off"
    }
}